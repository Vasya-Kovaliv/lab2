class BigNumber:
    def __init__(self):
        self.number = []

    def set_number_from_hex(self, hex_string):
        self.number = []
        hex_string = hex_string.strip()
        hex_string = hex_string.replace("0x", "")  
        if len(hex_string) % 2 != 0:  
            hex_string = "0" + hex_string  

        for i in range(0, len(hex_string), 2):
            byte = hex_string[i:i + 2]
            self.number.append(int(byte, 16))

    def get_number_as_hex(self):
        hex_string = ""
        for byte in self.number:
            hex_string += format(byte, "02x")
        return hex_string

number = BigNumber()
number.set_number_from_hex("A1B2C3D4E5F6")
print(number.get_number_as_hex())

